
.title {
	text-align: center;
	position: relative;
	margin-top: 20rpx;
	margin-bottom: 20rpx;
}

.back-btn {
	height:70rpx;
	background:#3CB371;
	color:white;
	font-size:30rpx;
	border-radius:6rpx;
}

.canvas {
    width: 100%;
    height: 200px;
}

.messageText{
  font-size:26rpx;
  color:#333;
  margin-top:60rpx;
  margin-left:30rpx;
  margin-right:30rpx;
}

.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 120rpx;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9 !important;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 120rpx;
  align-items: center;
}
.index{
	height:120rpx;
	width:100%;
	border-top:1px solid #ddd;
}
.indexTr{
	background:#3366FF;
	width: 150rpx; 
	display: inline-block;
  	color: #fff;
  	height: 120rpx;
  	text-align:center;
  	line-height:120rpx;
}
.indexTd{
	background:white;
	width: 150rpx; 
	display: inline-block;
  	color: #fff;
  	height: 120rpx;
  	text-align:center;
  	line-height:120rpx;
  	color:#4A4A4B;
}
.bg-g2{
  background: #E6F3F9;
  color:#4A4A4B;
}